{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s --copy-files && rm -rf ./build/tests",
    "clean": "rm -rf build",
    "start": "npm run build && node ./build/bin/www",
    "start:dev": "nodemon --exec babel-node ./src/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1"
  }
}
